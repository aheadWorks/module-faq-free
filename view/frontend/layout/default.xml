<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Aheadworks_FaqFree::css/style.css" />
    </head>
    <body>
        <referenceBlock name="footer_links">
            <block class="Aheadworks\FaqFree\Block\Link" name="footer_faq_link" ifconfig="faq/general/footer_menu_link_enabled" template="Aheadworks_FaqFree::link.phtml"/>
        </referenceBlock>
        <referenceBlock name="header.links">
            <block class="Aheadworks\FaqFree\Block\Link" name="top_faq_link" ifconfig="faq/general/top_menu_link_enabled" template="Aheadworks_FaqFree::link.phtml"/>
        </referenceBlock>
        <referenceBlock name="catalog.topnav">
            <block class="Aheadworks\FaqFree\Block\MenuItem" name="topnav_faq_menu_item" template="Aheadworks_FaqFree::menu_item.phtml"/>
        </referenceBlock>
    </body>
</page>
